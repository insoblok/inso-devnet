      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 2 },
      "targets": [
        { "expr": "inso_sequencer_lane_tx_count{lane=\"fast\"}", "legendFormat": "Fast" },
        { "expr": "inso_sequencer_lane_tx_count{lane=\"standard\"}", "legendFormat": "Standard" },
        { "expr": "inso_sequencer_lane_tx_count{lane=\"slow\"}", "legendFormat": "Slow" }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
