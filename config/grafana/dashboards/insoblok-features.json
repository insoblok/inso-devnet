    },
    {
      "title": "Gas Limit Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        { "expr": "inso_sequencer_adaptive_gas_limit", "legendFormat": "Current Gas Limit" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "custom": { "fillOpacity": 10 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Min" }, "properties": [{ "id": "custom.lineStyle", "value": { "fill": "dash" } }] },
          { "matcher": { "id": "byName", "options": "Max" }, "properties": [{ "id": "custom.lineStyle", "value": { "fill": "dash" } }] }
        ]
      }
    },
    {
      "title": "Utilization EMA Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        { "expr": "inso_sequencer_adaptive_utilization_bps / 10000", "legendFormat": "Utilization EMA" }
      ],
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 1, "custom": { "fillOpacity": 30 } } }
    },
    {
