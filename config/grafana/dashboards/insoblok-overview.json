      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "title": "RPC Requests",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        { "expr": "rate(inso_sequencer_rpc_requests_total[5m])", "legendFormat": "Requests/5m" },
        { "expr": "rate(inso_sequencer_rpc_errors_total[5m])", "legendFormat": "Errors/5m" }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "title": "Validator Attestations",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        { "expr": "rate(inso_validator_attestations_created_total[5m])", "legendFormat": "Created/5m" },
        { "expr": "rate(inso_validator_attestations_verified_total[5m])", "legendFormat": "Verified/5m" },
        { "expr": "rate(inso_validator_attestations_failed_total[5m])", "legendFormat": "Failed/5m" }
      ]
    },
    {
      "title": "Validator Sovereignty",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 12, "y": 20 },
