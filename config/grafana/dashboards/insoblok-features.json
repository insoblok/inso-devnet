    },
    {
      "title": "Current Max Tx/Block",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 16 },
      "targets": [{ "expr": "inso_sequencer_adaptive_max_tx", "legendFormat": "Max Tx" }],
