      "fieldConfig": { "defaults": { "decimals": 4, "thresholds": { "steps": [{ "color": "gray", "value": null }, { "color": "orange", "value": 0.15 }, { "color": "silver", "value": 0.40 }, { "color": "gold", "value": 0.65 }, { "color": "cyan", "value": 0.85 }] } } }
    },
    {
      "title": "Validator Peers",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 20 },
