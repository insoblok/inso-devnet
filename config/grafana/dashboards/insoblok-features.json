      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" } } }
    },
    {
      "title": "Lane Gas Throughput",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
